<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wxthxy.zj.dao.DesignproblemDAO" >
    <select id="getCount" resultType="Integer">
        select count(1) from Designproblem;
    </select>
    <select id="getAllDesignproblems" resultType="com.wxthxy.zj.entity.Designproblem">
        select qt.type as type,qt.level as level,dp.* from Designproblem dp left join questiontype qt
                                                                                 on dp.questiontypeid=qt.id;
    </select>
</mapper>