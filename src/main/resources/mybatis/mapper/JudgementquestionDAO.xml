<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wxthxy.zj.dao.JudgementquestionDAO" >
    <select id="getCount" resultType="Integer">
        select count(1) from Judgementquestion;
    </select>
    <select id="getAllJudgementquestions" resultType="com.wxthxy.zj.entity.Judgementquestion">
        select qt.type as type,qt.level as level,jq.* from Judgementquestion jq left join questiontype qt
                                                                                      on jq.questiontypeid=qt.id;
    </select>
</mapper>